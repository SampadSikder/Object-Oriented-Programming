<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="AgeAnchor" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Age">
   <children>
      <Button fx:id="Logout" layoutX="453.0" layoutY="340.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
      <Label layoutX="37.0" layoutY="112.0" text="Enter Your Message">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label fx:id="AgeLabel" layoutX="214.0" layoutY="52.0" text="Welcome!">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <TextField fx:id="TextField" layoutX="63.0" layoutY="175.0" />
      <Button fx:id="SendMessage" layoutX="111.0" layoutY="213.0" mnemonicParsing="false" onAction="#submit" text="Message" />
      <Label fx:id="ReplyLabel" layoutX="49.0" layoutY="251.0" prefHeight="40.0" prefWidth="430.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <CheckBox fx:id="myCheckBox" layoutX="106.0" layoutY="344.0" mnemonicParsing="false" onAction="#change" text="On/Off" />
      <ImageView fx:id="ImageCheck" fitHeight="150.0" fitWidth="125.0" layoutX="481.0" layoutY="196.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@raju2.jpg" />
         </image>
      </ImageView>
      <RadioButton fx:id="r1" layoutX="366.0" layoutY="179.0" mnemonicParsing="false" onAction="#getFood" text="Pizza">
         <toggleGroup>
            <ToggleGroup fx:id="Food" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="r2" layoutX="480.0" layoutY="179.0" mnemonicParsing="false" onAction="#getFood" text="Burger" toggleGroup="$Food" />
      <Label fx:id="RadioLabel" layoutX="422.0" layoutY="135.0" text="Select Food" />
   </children>
</AnchorPane>
